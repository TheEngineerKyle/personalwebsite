{
    "cleanUrls": true,
    "redirects": [
        {
            "source": "/404",
            "destination": "/404.html",
            "statusCode": 404
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/.*",
            "status": 404,
            "dest": "/404.html"
        }
    ]
}